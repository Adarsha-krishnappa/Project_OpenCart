<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite  thread-count="2" name="mysuite" >
	
	 <listeners>
		<listener class-name="utilities.ExtentReportManager"/>
	</listeners>
	
	<test name="mytest1" thread-count="5">
		<parameter name="os" value="Windows"/>
		<parameter name="browser" value="chrome"></parameter>
		<classes>
			<class name="testCases.TC_001_AccountRegistrationTest"></class>
		</classes>
	</test>
	
	<test name="mytest2">
		<parameter name="os" value="Windows"/>
		<parameter name="browser" value="edge"></parameter>
		<classes>
			<class name="testCases.TC_002_My_Account_Login"></class>
		</classes>
	</test>
	
	<test name="mytest3">
		<parameter name="os" value="Windows"/>
		<parameter name="browser" value="firefox"></parameter>
		<classes>
			<class name="testCases.TC_003_LoginDataDrivenTest"></class>
		</classes>
	</test>
	
</suite>